<!DOCTYPE html>
<html>
<head>
  <title>Homepage</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<!-- Start Header -->
<div id="header">
  <div id="logo">
  <%= image_tag "logo.png" %>
  </div>
  <div id="sitetitle">
  <h1>Pete's Site</h1>
  </div>
</div>
<!-- End Header -->
<%= yield %>
<!-- Start Footer -->
<div id="footer">
  <div id="copyright">
  <p>Copyright Peter Mellett 2011</p>
  </div>
</div>
<!-- End Header -->
</body>
</html>
